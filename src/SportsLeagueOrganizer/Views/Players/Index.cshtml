@model IEnumerable<SportsLeagueOrganizer.Models.Player>

@{
    ViewData["Title"] = "Players";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>All Players</h2>
<hr />

@foreach (var player in Model)
{
    <p>
        @Html.DisplayFor(modelPlayer => player.Name) -
        @Html.DisplayFor(modelPlayer => player.Phone) | @Html.ActionLink("Player Details", "Details", new { id = player.PlayerId })
    </p>
    
}
